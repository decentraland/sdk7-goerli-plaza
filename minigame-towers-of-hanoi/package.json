{
  "name": "dcl-project",
  "version": "1.0.0",
  "description": "SDK7 Template scene",
  "scripts": {
    "start": "sdk-commands start",
    "build:ci": "sdk-commands build",
    "deploy:prod": "sdk-commands deploy -t linker-server.decentraland.org --skip-validations true",
    "deploy:testing": "sdk-commands deploy -t linker-server.decentraland.today --skip-validations true",
    "deploy:world": "sdk-commands deploy --target-content https://worlds-content-server.decentraland.org",
    "deploy": "sdk-commands deploy",
    "build": "sdk-commands build",
    "upgrade-sdk": "npm install --save-dev @dcl/sdk@latest",
    "upgrade-sdk:next": "npm install --save-dev @dcl/sdk@next"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "trailingComma": "none"
  },
  "devDependencies": {
    "@dcl-sdk/mini-games": "^1.0.0-20240814125920.commit-7a79815",
    "@dcl-sdk/utils": "^1.2.8",
    "@dcl/asset-packs": "^1.20.0",
    "@dcl/sdk": "next"
  }
}