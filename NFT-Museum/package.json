  "version": "1.0.0",
  "dependencies": {
    "@dcl-sdk/utils": "latest",
    "dcl-ui-toolkit": "latest"
  },
  "devDependencies": {
    "@dcl/asset-packs": "^1.20.1",
    "@dcl/js-runtime": "7.5.7",
    "@dcl/sdk": "^7.5.7"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  },
  "prettier": {
