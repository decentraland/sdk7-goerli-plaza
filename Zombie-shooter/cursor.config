{
  "$schema": "https://raw.githubusercontent.com/getcursor/cursor-config/main/schema/cursor.schema.json",
  "context": {
    "include": [
      "https://github.com/decentraland/documentation/blob/main/ai-sdk-context/sdk7-complete-reference.md",
